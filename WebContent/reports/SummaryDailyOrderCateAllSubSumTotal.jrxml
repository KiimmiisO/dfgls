	<queryString><![CDATA[SELECT 
	ISNULL(SUM(CASE WHEN OI.ORDER_ITEM_CATEGORY_CODE = 'XRY' THEN AMOUNT_BEF_DISCOUNT END  ) , 0)  -
	ISNULL(SUM(CASE WHEN OI.ORDER_ITEM_CATEGORY_CODE = 'XRY' THEN AMOUNT_OF_DISCOUNT END  ) , 0)
	AS XRY ,
	ISNULL(SUM(CASE WHEN OI.ORDER_ITEM_CATEGORY_CODE = 'XRY-B' THEN AMOUNT_BEF_DISCOUNT END ) , 0) -
	ISNULL(SUM(CASE WHEN OI.ORDER_ITEM_CATEGORY_CODE = 'XRY-B' THEN AMOUNT_OF_DISCOUNT END ) , 0)
	AS XRYB ,
	ISNULL(SUM(CASE WHEN OI.ORDER_ITEM_CATEGORY_CODE = 'XRY-C' THEN AMOUNT_BEF_DISCOUNT END ) , 0) -
	ISNULL(SUM(CASE WHEN OI.ORDER_ITEM_CATEGORY_CODE = 'XRY-C' THEN AMOUNT_OF_DISCOUNT END ) , 0)
	AS XRYC , 
	ISNULL(SUM(CASE WHEN OI.ORDER_ITEM_CATEGORY_CODE = 'XRY-M' THEN AMOUNT_BEF_DISCOUNT END ) , 0) -
	ISNULL(SUM(CASE WHEN OI.ORDER_ITEM_CATEGORY_CODE = 'XRY-M' THEN AMOUNT_OF_DISCOUNT END ) , 0)
	AS XRYM , 
	ISNULL(SUM(CASE WHEN OI.ORDER_ITEM_CATEGORY_CODE = 'XRY-U' THEN AMOUNT_BEF_DISCOUNT END ) , 0) -
	ISNULL(SUM(CASE WHEN OI.ORDER_ITEM_CATEGORY_CODE = 'XRY-U' THEN AMOUNT_OF_DISCOUNT END ) , 0) 
	AS XRYU ,
	ISNULL(SUM(CASE WHEN OI.ORDER_ITEM_CATEGORY_CODE IN ( 'XRY' , 'XRY-B' , 'XRY-C' , 'XRY-M' , 'XRY-U' ) THEN AMOUNT_BEF_DISCOUNT END) , 0) - 
	ISNULL(SUM(CASE WHEN OI.ORDER_ITEM_CATEGORY_CODE IN ( 'XRY' , 'XRY-B' , 'XRY-C' , 'XRY-M' , 'XRY-U' ) THEN AMOUNT_OF_DISCOUNT END) , 0)
	AS EXPENSE_XRAY ,
	'CODE' AS CODE , 'NAME_THAI' AS NAME  , 'DEPARTMENT_CODE' AS DEPARTMENT , 'A' AS C
FROM DOCTOR D 
INNER JOIN TRN_DAILY TD ON 
	D.CODE = TD.DOCTOR_CODE AND D.HOSPITAL_CODE  = TD.HOSPITAL_CODE 
INNER JOIN ORDER_ITEM OI ON TD.ORDER_ITEM_CODE = OI.CODE  AND  TD.HOSPITAL_CODE =  OI.HOSPITAL_CODE
WHERE  
D.DOCTOR_TYPE_CODE = 'XRY' AND TD.GUARANTEE_TERM_YYYY = '' AND TD.NOTE = 'XM' AND  
TD.IS_ONWARD <> 'Y' AND 
TD.PAYOR_OFFICE_CODE LIKE $P{payor_office_code} AND  
( TD.TRANSACTION_DATE BETWEEN $P{from_date} AND $P{to_date} ) AND 
TD.INVOICE_TYPE <> 'ORDER' AND 
D.DOCTOR_PROFILE_CODE LIKE $P{doctor_profile_code} AND 
D.CODE LIKE $P{doctor_code} AND 
D.DEPARTMENT_CODE LIKE $P{doctor_department} AND 
D.DOCTOR_CATEGORY_CODE LIKE $P{doctor_category} AND 
D.HOSPITAL_CODE LIKE $P{hospital_code} AND
D.ACTIVE = '1' AND 
OI.ORDER_ITEM_CATEGORY_CODE IN ('XRY' , 'XRY-B' , 'XRY-C' , 'XRY-M' , 'XRY-U') AND
OI.ORDER_ITEM_CATEGORY_CODE LIKE $P{order_category}
HAVING SUM(AMOUNT_OF_DISCOUNT) > 0]]></queryString>

