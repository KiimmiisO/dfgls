	</parameter>
	<queryString><![CDATA[SELECT DISTINCT DOCTOR_PROFILE_CODE, DOCTOR_PROFILE_NAME, DOCTOR_CODE, DOCTOR_NAME,
CASE WHEN (SELECT ACTIVE FROM DOCTOR WHERE ACTIVE = '' AND CODE = DOCTOR_CODE 
AND HOSPITAL_CODE LIKE $P{hospital_code}) IS NULL THEN 'NEW DOCTOR' 
ELSE 'UPDATE DOCTOR' END AS INFO
FROM INT_HIS_BILL
WHERE (DOCTOR_CODE NOT IN (SELECT CODE FROM DOCTOR WHERE HOSPITAL_CODE LIKE $P{hospital_code})
OR (SELECT ACTIVE FROM DOCTOR WHERE ACTIVE = '' AND CODE = DOCTOR_CODE) = '')
AND BILL_DATE BETWEEN $P{date_from} AND $P{date_to}
AND HOSPITAL_CODE LIKE $P{hospital_code}
ORDER BY INFO, DOCTOR_PROFILE_CODE, DOCTOR_CODE]]></queryString>

