	<queryString><![CDATA[SELECT 	BANK.CODE as Bankcode, BANK.DESCRIPTION_THAI as Bankname, BANK_BRANCH.CODE as Branchcode, 
	BANK_BRANCH.DESCRIPTION_THAI as Branchname, BANK_TMB_MEDIA_CLEARING.RECEIVING_ACCOUNT_NO, 
	DOCTOR.CODE as Doctorcode, DOCTOR.NAME_THAI as Doctorname, DOCTOR.DOCTOR_PROFILE_CODE as Doctorgroup,
	CONVERT(INT,BANK_TMB_MEDIA_CLEARING.TRANSFER_AMOUNT) / 100.00 AS AMOUNT, 
	BANK_TMB_MEDIA_CLEARING.EFFECTIVE_DATE
FROM    BANK_TMB_MEDIA_CLEARING
LEFT OUTER JOIN DOCTOR ON BANK_TMB_MEDIA_CLEARING.DOCTOR_CODE=DOCTOR.CODE
and BANK_TMB_MEDIA_CLEARING.HOSPITAL_CODE=DOCTOR.HOSPITAL_CODE
LEFT OUTER JOIN BANK ON BANK_TMB_MEDIA_CLEARING.RECEIVING_BANK_CODE=BANK.CODE
LEFT OUTER JOIN BANK_BRANCH ON BANK_TMB_MEDIA_CLEARING.RECEIVING_BRANCH_CODE=BANK_BRANCH.CODE
AND BANK_TMB_MEDIA_CLEARING.RECEIVING_BANK_CODE=BANK_BRANCH.BANK_CODE
WHERE BANK_TMB_MEDIA_CLEARING.YYYY like $P{year} and BANK_TMB_MEDIA_CLEARING.MM like $P{month}
AND BANK_TMB_MEDIA_CLEARING.HOSPITAL_CODE LIKE $P{hospital_code}
--AND BANK_TMB_MEDIA_CLEARING.RECEIVING_BANK_CODE <> '025'
ORDER BY DOCTOR.DOCTOR_PROFILE_CODE, DOCTOR.CODE]]></queryString>

