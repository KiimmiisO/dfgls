	<queryString><![CDATA[SELECT S.HOSPITAL_CODE, S.INVOICE_NO, S.INVOICE_DATE, S.HN_NO, S.PATIENT_NAME,
S.PAYOR_OFFICE_CODE, S.PAYOR_OFFICE_NAME, S.DOCTOR_CODE, D.NAME_THAI, 
SUM(S.AMOUNT_AFT_DISCOUNT) AS AMOUNT_AFT_DISCOUNT, SUM(S.DR_AMT) AS DR_AMT,
SUM(S.DR_TAX_400+S.DR_TAX_401+S.DR_TAX_402+S.DR_TAX_406) AS DR_TAX_AMT
FROM TRN_DAILY S LEFT OUTER JOIN DOCTOR D 
ON (S.DOCTOR_CODE = D.CODE AND S.HOSPITAL_CODE = D.HOSPITAL_CODE)
WHERE (S.INVOICE_DATE BETWEEN $P{from_date} AND $P{to_date}) AND
S.DOCTOR_CODE LIKE $P{doctor} AND INVOICE_TYPE <> 'ORDER' AND
S.ACTIVE = '1' AND D.ACTIVE = '1' AND S.IS_PAID <> 'N' AND
--(S.PAY_BY_CASH = 'N' AND S.PAY_BY_AR = 'N' AND S.PAY_BY_DOCTOR = 'N' AND
--S.PAY_BY_PAYOR = 'N' AND S.PAY_BY_CASH_AR = 'N') AND
(S.BATCH_NO = '' OR S.BATCH_NO IS NULL) AND
(S.RECEIPT_DATE <= $P{as_of_date} AND S.INVOICE_DATE <= $P{as_of_date}) AND
S.HOSPITAL_CODE LIKE $P{hospital_code}
GROUP BY S.HOSPITAL_CODE, S.INVOICE_NO, S.INVOICE_DATE, S.HN_NO, S.PATIENT_NAME,
S.PAYOR_OFFICE_CODE, S.PAYOR_OFFICE_NAME, S.DOCTOR_CODE, D.NAME_THAI
ORDER BY S.DOCTOR_CODE, S.INVOICE_NO, S.INVOICE_DATE]]></queryString>

