	<queryString><![CDATA[SELECT Q.HOSPITAL_CODE, D.DOCTOR_PROFILE_CODE, Q.GUARANTEE_DR_CODE, D.NAME_THAI, 
SUM(GUARANTEE_AMOUNT) AS GUARANTEE_AMOUNT, SUM(AMOUNT_AFT_DISCOUNT) AS AMOUNT_AFT_DISCOUNT, 
SUM(DR_AMT) AS DR_AMT, SUM(ABSORB_AMOUNT) AS ABSORB_AMOUNT ,SUM(ABSORB_REMAIN) AS ABSORB_REMAIN
FROM (SELECT T.HOSPITAL_CODE, 
T.GUARANTEE_DR_CODE, 0 AS GUARANTEE_AMOUNT, SUM(AMOUNT_AFT_DISCOUNT) AS AMOUNT_AFT_DISCOUNT, 
SUM(DR_AMT) AS DR_AMT, 0 AS ABSORB_AMOUNT, 0 AS ABSORB_REMAIN FROM TRN_PAYMENT T 
WHERE T.HOSPITAL_CODE = $P{hospital_code}
AND T.GUARANTEE_TERM_YYYY <> '' AND T.GUARANTEE_TYPE = 'DLY' 
AND T.YYYY+MM = $P{year}+$P{month}
AND T.GUARANTEE_NOTE NOT LIKE '%EXTRA%' GROUP BY T.HOSPITAL_CODE, T.GUARANTEE_DR_CODE 
UNION 
SELECT HOSPITAL_CODE, GUARANTEE_DR_CODE, SUM(GUARANTEE_AMOUNT) AS GUARANTEE_AMOUNT, 
0 AS AMOUNT_AFT_DISCOUNT, 0 AS DR_AMT, SUM(HP402_ABSORB_AMOUNT) AS ABSORB_AMOUNT,
SUM(DF_ABSORB_AMOUNT) AS ABSORB_REMAIN
FROM STP_GUARANTEE 
WHERE HOSPITAL_CODE = $P{hospital_code} 
AND ACTIVE = '1' AND GUARANTEE_TYPE_CODE = 'DLY' 
AND YYYY+MM = $P{year}+$P{month} 
GROUP BY HOSPITAL_CODE, GUARANTEE_DR_CODE ) 
Q LEFT OUTER JOIN DOCTOR D ON Q.GUARANTEE_DR_CODE = D.CODE 
AND Q.HOSPITAL_CODE = D.HOSPITAL_CODE 
WHERE Q.HOSPITAL_CODE = $P{hospital_code} 
AND D.DEPARTMENT_CODE LIKE $P{doctor_department}
GROUP BY Q.HOSPITAL_CODE, D.DOCTOR_PROFILE_CODE, Q.GUARANTEE_DR_CODE, D.NAME_THAI 
ORDER BY D.DOCTOR_PROFILE_CODE, Q.GUARANTEE_DR_CODE DESC]]></queryString>

