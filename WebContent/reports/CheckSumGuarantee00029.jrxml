	<queryString><![CDATA[SELECT S.HOSPITAL_CODE, CASE WHEN S.ADMISSION_TYPE_CODE = 'I' THEN 'IPD' ELSE 'OPD' END AS ADMISSION_TYPE_CODE,
S.TRANSACTION_TYPE, S.TRANSACTION_MODULE, S.INVOICE_NO, S.INVOICE_DATE, 
S.VERIFY_DATE, S.VERIFY_TIME, S.RECEIPT_TYPE_CODE, S.HN_NO, S.LINE_NO,
S.PATIENT_NAME, S.EPISODE_NO, S.PAYOR_OFFICE_CODE, S.PAYOR_OFFICE_NAME, S.ORDER_ITEM_CODE, S.DOCTOR_CATEGORY_CODE,
S.ORDER_ITEM_DESCRIPTION, S.GUARANTEE_DR_CODE, S.DOCTOR_CODE, D.NAME_THAI, S.AMOUNT_AFT_DISCOUNT, 
S.DOCTOR_DEPARTMENT_CODE, S.NOR_ALLOCATE_AMT, S.HP_AMT, S.GUARANTEE_PAID_AMT, S.DR_AMT,
CASE WHEN S.GUARANTEE_NOTE LIKE 'ABSORB GUARANTEE%' THEN 'CREDIT ADVANCE PAYMENT' ELSE 
CASE WHEN S.GUARANTEE_NOTE = 'ABSORB SOME GUARANTEE' THEN 'CREDIT ADVANCE SOME PAYMENT' ELSE S.GUARANTEE_NOTE END END AS GUARANTEE_NOTE,
CASE WHEN S.GUARANTEE_PAID_AMT > 0 THEN S.GUARANTEE_PAID_AMT ELSE S.DR_AMT END AS GUARANTEE_AMT,
S.DR_TAX_400+S.DR_TAX_401+S.DR_TAX_402+S.DR_TAX_406 AS DR_TAX_AMT, S.TAX_TYPE_CODE, S.NOR_ALLOCATE_PCT,
CASE WHEN LEN(S.GUARANTEE_CODE) < 8 THEN S.GUARANTEE_CODE+'01000000' ELSE S.GUARANTEE_CODE END AS GUARANTEE_CODE
FROM TRN_DAILY S LEFT OUTER JOIN DOCTOR D ON S.DOCTOR_CODE = D.CODE
AND S.HOSPITAL_CODE = D.HOSPITAL_CODE
WHERE S.GUARANTEE_TERM_MM = $P{month} AND
S.GUARANTEE_TERM_YYYY = $P{year} AND
(S.GUARANTEE_DR_CODE BETWEEN $P{from_doctor} and $P{to_doctor}) AND
S.GUARANTEE_AMT > 0 AND
S.GUARANTEE_NOTE <> '' AND
S.ORDER_ITEM_ACTIVE = '1' AND S.ACTIVE = '1' AND
S.HOSPITAL_CODE LIKE $P{hospital_code}
ORDER BY S.GUARANTEE_DR_CODE, S.GUARANTEE_CODE, S.GUARANTEE_NOTE DESC, S.TRANSACTION_DATE, S.INVOICE_NO, S.LINE_NO]]></queryString>

