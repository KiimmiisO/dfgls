	<queryString><![CDATA[SELECT S.HOSPITAL_CODE, CASE WHEN S.ADMISSION_TYPE_CODE = 'I' THEN 'IPD' ELSE 'OPD' END AS ADMISSION_TYPE_CODE,
S.EXECUTE_TIME AS VERIFY_TIME, S.EXECUTE_DATE AS VERIFY_DATE, S.TRANSACTION_TYPE, S.TRANSACTION_MODULE, S.INVOICE_NO, S.INVOICE_DATE, 
S.RECEIPT_TYPE_CODE, S.HN_NO, S.LINE_NO, S.PATIENT_NAME, S.EPISODE_NO, S.PAYOR_OFFICE_CODE, S.PAYOR_OFFICE_NAME, 
S.ORDER_ITEM_CODE, S.DOCTOR_CATEGORY_CODE, S.ORDER_ITEM_DESCRIPTION, OI.ORDER_ITEM_CATEGORY_CODE, OIG.DESCRIPTION_THAI , S.DOCTOR_CODE, D.NAME_THAI, 
S.AMOUNT_BEF_DISCOUNT AS AMOUNT_AFT_DISCOUNT, S.AMOUNT_OF_DISCOUNT,  S.DOCTOR_DEPARTMENT_CODE, S.NOR_ALLOCATE_AMT, S.DR_AMT, S.HP_AMT, 
CASE WHEN S.TAX_TYPE_CODE = '406' THEN S.DR_TAX_406 ELSE S.DR_TAX_400+S.DR_TAX_401+S.DR_TAX_402 END 
AS DR_TAX_AMT, S.TAX_TYPE_CODE, S.NOR_ALLOCATE_PCT
FROM TRN_DAILY S LEFT OUTER JOIN DOCTOR D ON S.DOCTOR_CODE = D.CODE AND S.HOSPITAL_CODE = D.HOSPITAL_CODE
LEFT OUTER JOIN ORDER_ITEM OI ON S.ORDER_ITEM_CODE = OI.CODE AND S.HOSPITAL_CODE = OI.HOSPITAL_CODE
LEFT OUTER JOIN ORDER_ITEM_CATEGORY OIG ON OI.ORDER_ITEM_CATEGORY_CODE = OIG.CODE AND OI.HOSPITAL_CODE = OIG.HOSPITAL_CODE
WHERE (S.TRANSACTION_DATE BETWEEN $P{from_date} AND $P{to_date}) AND 
D.DOCTOR_TYPE_CODE = 'XRY' AND 
S.ADMISSION_TYPE_CODE LIKE $P{admission_type} AND 
S.NOTE = 'XM' AND 
D.DOCTOR_PROFILE_CODE LIKE $P{doctor_profile_code} AND 
S.DOCTOR_CODE LIKE $P{doctor_code} AND 
D.DEPARTMENT_CODE LIKE $P{doctor_department} AND 
S.PAYOR_OFFICE_CODE LIKE $P{payor_office_code} AND
--S.ACTIVE = '1' AND 
S.ORDER_ITEM_CODE LIKE $P{order_item} AND 
S.INVOICE_NO LIKE $P{invoice_no} AND 
D.DOCTOR_CATEGORY_CODE LIKE $P{doctor_category} AND
OI.ORDER_ITEM_CATEGORY_CODE IN ( 'XRY' , 'XRY-B' , 'XRY-C' , 'XRY-M' , 'XRY-U' ) AND 
S.IS_WRITE_OFF LIKE $P{doc_type} AND 
OI.ORDER_ITEM_CATEGORY_CODE LIKE $P{order_category} AND
S.HOSPITAL_CODE LIKE $P{hospital_code} AND 
S.INVOICE_TYPE <>  'ORDER' AND 
S.IS_ONWARD <> 'Y'
ORDER BY S.DOCTOR_CODE , OI.ORDER_ITEM_CATEGORY_CODE , S.INVOICE_DATE , S.INVOICE_NO]]></queryString>

