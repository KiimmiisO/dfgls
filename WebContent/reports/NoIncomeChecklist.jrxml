	<queryString><![CDATA[SELECT DOCTOR_CODE, NAME_THAI, SUM(DR_AMT) AS DR_AMT, SUM(EXPENSE) AS EXPENSE, SUM(DR_AMT)-SUM(EXPENSE) AS AMOUNT
FROM (
SELECT DOCTOR_CODE, D.NAME_THAI, DR_AMT, 0 AS EXPENSE 
FROM TRN_PAYMENT T LEFT OUTER JOIN DOCTOR D
ON T.HOSPITAL_CODE = D.HOSPITAL_CODE AND T.DOCTOR_CODE = D.CODE
WHERE T.HOSPITAL_CODE = $P{hospital_code}
AND ORDER_ITEM_ACTIVE = '1'
AND T.ACTIVE = '1'
AND IS_PAID = 'Y'
AND TRANSACTION_DATE LIKE $P{year}+$P{month}+'%'
AND BATCH_NO = ''
AND YYYY+MM = $P{year}+$P{month}
UNION
SELECT DOCTOR_CODE, D.NAME_THAI, 0 AS DR_AMT, AMOUNT*EXPENSE_SIGN AS EXPENSE 
FROM TRN_EXPENSE_DETAIL T LEFT OUTER JOIN DOCTOR D
ON T.HOSPITAL_CODE = D.HOSPITAL_CODE AND T.DOCTOR_CODE = D.CODE
WHERE T.HOSPITAL_CODE = $P{hospital_code}
AND BATCH_NO = ''
AND YYYY+MM = $P{year}+$P{month}
) AS Q
GROUP BY DOCTOR_CODE, NAME_THAI
HAVING SUM(DR_AMT)+SUM(EXPENSE) < 0]]></queryString>

