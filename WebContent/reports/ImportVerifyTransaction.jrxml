	<queryString><![CDATA[SELECT IHV.HOSPITAL_CODE, IHV.BILL_NO, IHV.LINE_NO, IHV.DOCTOR_CODE, IHV.VERIFIED_DATE,
IHV.VERIFIED_TIME, D.NAME_THAI , HP.DESCRIPTION_THAI
FROM INT_HIS_VERIFY IHV 
LEFT OUTER JOIN DOCTOR D ON IHV.DOCTOR_CODE = D.CODE
INNER JOIN HOSPITAL HP ON IHV.HOSPITAL_CODE = HP.CODE 
WHERE (VERIFIED_DATE BETWEEN $P{from_date} AND $P{to_date})
AND IHV.HOSPITAL_CODE LIKE $P{hospital_code}
AND IHV.DOCTOR_CODE LIKE $P{doctor_code}]]></queryString>

