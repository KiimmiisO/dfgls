	<queryString><![CDATA[SELECT INF.HOSPITAL_CODE, INF.BILL_NO, INF.LINE_NO, INF.DOCTOR_CODE, D.NAME_THAI, INF.VERIFIED_DATE,
INF.VERIFIED_TIME, INF.TRANSACTION_DATE
FROM INT_HIS_VERIFY INF LEFT OUTER JOIN DOCTOR D ON INF.DOCTOR_CODE = D.CODE AND INF.HOSPITAL_CODE = D.HOSPITAL_CODE
WHERE INF.HOSPITAL_CODE LIKE $P{hospital_code}
AND INF.TRANSACTION_DATE between $P{from_date} and $P{to_date}
AND D.DEPARTMENT_CODE LIKE $P{doctor_department}
AND INF.LINE_NO IN (SELECT LINE_NO FROM TRN_DAILY WHERE INVOICE_TYPE = 'ORDER' AND HOSPITAL_CODE LIKE $P{hospital_code})
AND INF.LINE_NO+'V' NOT IN (SELECT LINE_NO FROM TRN_DAILY WHERE INVOICE_TYPE = 'RESULT' AND HOSPITAL_CODE LIKE $P{hospital_code})
ORDER BY INF.LINE_NO]]></queryString>

