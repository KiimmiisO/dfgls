	<queryString><![CDATA[SELECT DR.DOCTOR_PROFILE_CODE, DP.NAME_THAI DOCTOR_PROFILE_NAME, DR.CODE DOCTOR_CODE,
DR.NAME_THAI AS DOCTOR_NAME,'UPDATE DOCTOR INFO.' AS INFO,  DR.USER_ID , DR.UPDATE_DATE, DR.UPDATE_TIME
, HP.DESCRIPTION_THAI
FROM DOCTOR DR 
LEFT OUTER JOIN DOCTOR_PROFILE DP ON
DR.DOCTOR_PROFILE_CODE = DP.CODE AND DR.HOSPITAL_CODE = DP.HOSPITAL_CODE
INNER JOIN HOSPITAL HP ON DR.HOSPITAL_CODE = HP.CODE
WHERE (DR.ACTIVE = '' OR DR.ACTIVE IS NULL OR DR.ACTIVE = 'NULL')
AND DR.HOSPITAL_CODE LIKE $P{hospital_code}
ORDER BY DR.DOCTOR_PROFILE_CODE, DR.CODE]]></queryString>

